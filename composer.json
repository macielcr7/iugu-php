{
    "name": "eloca/iugu-php",
    "description": "Package PHP para integração com a API Iugu (Clean Architecture, PSR-4, PHP 8.1)",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Iugu\\": "src/"
        },
        "files": [
            "src/Support/config.php",
            "src/Support/env.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Maciel Sousa",
            "email": "macielcr7@gmail.com"
        }
    ]
} 